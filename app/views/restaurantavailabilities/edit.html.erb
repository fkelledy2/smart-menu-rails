<div class="page-header">
  <div class="row">
    <div class="col-sm-10">
        <h1><%=t(".restaurantavailability")%></h1>
    </div>
    <div class="col-sm-2">
        <% if @restaurantavailability.persisted? %>
          <%= link_to t(".cancel"), @restaurantavailability, class: "float-end btn btn-light" %>
        <% else %>
          <%= link_to t(".cancel"), restaurantavailabilies_path, class: "float-end btn btn-light" %>
        <% end %>
    </div>
  </div>
</div>
<%= render 'form', restaurantavailability: @restaurantavailability %>
