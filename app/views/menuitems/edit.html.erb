<div class="page-header">
  <div class="row">
    <div class="col-sm-10">
          <h1>Edit Menu Item</h1>
    </div>
    <div class="col-sm-2">
        <% if @menuitem.persisted? %>
          <%= link_to "Cancel", @menuitem, class: "float-end btn btn-light" %>
        <% else %>
          <%= link_to "Cancel", menuitems_path, class: "float-end btn btn-light" %>
        <% end %>
    </div>
  </div>
</div>
<%= render 'form', menuitem: @menuitem %>


