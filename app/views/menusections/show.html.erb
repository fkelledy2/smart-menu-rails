<div class="page-header">
  <%= link_to menusections_path, class: 'btn btn-default' do %>
    All Menusections
  <% end %>
  <%= link_to edit_menusection_path(@menusection), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show menusection</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @menusection.name %></dd>
  <dt>Description:</dt>
  <dd><%= @menusection.description %></dd>
  <dt>Image:</dt>
  <dd><%= @menusection.image %></dd>
  <dt>Status:</dt>
  <dd><%= @menusection.status %></dd>
  <dt>Sequence:</dt>
  <dd><%= @menusection.sequence %></dd>
  <dt>Menu:</dt>
  <dd><%= @menusection.menu %></dd>
</dl>
