<%#
  Turbo Frame wrapper for menu section content
  Used when responding to Turbo Frame navigation requests
%>

<%= turbo_frame_tag 'menu_content' do %>
  <%= render "menus/sections/#{local_assigns[:partial_name]}",
      menu: local_assigns[:menu],
      restaurant: local_assigns[:restaurant],
      restaurant_menu: local_assigns[:restaurant_menu],
      read_only: local_assigns[:read_only] %>
<% end %>
