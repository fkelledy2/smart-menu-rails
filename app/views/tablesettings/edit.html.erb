<div class="page-header">
  <div class="row">
    <div class="col-sm-10">
          <h1>Edit Table Setting</h1>
    </div>
    <div class="col-sm-2">
        <% if @tablesetting.persisted? %>
          <%= link_to "Cancel", @tablesetting, class: "float-end btn btn-light" %>
        <% else %>
          <%= link_to "Cancel", tablesettings_path, class: "float-end btn btn-light" %>
        <% end %>
    </div>
  </div>
</div>
<%= render 'form', tablesetting: @tablesetting %>


