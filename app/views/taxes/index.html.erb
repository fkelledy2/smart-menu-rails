<div class="row">
  <div class="col-sm-6">
    <h1><%=t('.taxes')%></h1>
  </div>
  <div class="col-sm-6 text-end">
    <% if @futureParentRestaurant %>
      <%= link_to restaurant_path(id: @futureParentRestaurant.id), class: 'btn btn-sm btn-light' do %>
        <i class="bi bi-chevron-left"></i>
      <% end %>
    <% end %>
  <%= link_to new_tax_path, class: 'btn btn-sm btn-primary' do %>
    <i class="bi bi-plus"></i> Tax
  <% end %>
  </div>
</div>
<div style="padding-top:10px" class="table-borderless" id="tax-table">
</div>
