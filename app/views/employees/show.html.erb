<div class="page-header">
  <%= link_to employees_path, class: 'btn btn-default' do %>
    All Employees
  <% end %>
  <%= link_to edit_employee_path(@employee), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show employee</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @employee.name %></dd>
  <dt>Eid:</dt>
  <dd><%= @employee.eid %></dd>
  <dt>Image:</dt>
  <dd><%= @employee.image %></dd>
  <dt>Status:</dt>
  <dd><%= @employee.status %></dd>
  <dt>Restaurant:</dt>
  <dd><%= @employee.restaurant %></dd>
</dl>
